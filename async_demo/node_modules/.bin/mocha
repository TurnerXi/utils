#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_mocha@3.4.2@mocha/bin/mocha" "$@"
  ret=$?
else 
  node  "$basedir/../_mocha@3.4.2@mocha/bin/mocha" "$@"
  ret=$?
fi
exit $ret
